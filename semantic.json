{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src\\site\\",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist\\",
      "uncompressed": "dist\\components\\",
      "compressed": "dist\\components\\",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "version": "2.4.2",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "label",
    "list",
    "segment",
    "grid",
    "menu",
    "card",
    "item",
    "dimmer",
    "modal",
    "popup",
    "sticky",
    "transition",
    "state",
    "visibility"
  ]
}